I=1,2,3,4,5
C=1<=5;2<=5;3<=5;4<=5;5<=5
U=i++ =i=i+1;1+1=2;3;4;5
B=1;2;3;4;5

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <script>
        for(let i=10;i<=15;i++){
            console.log(i);
        }
    </script>
</body>
</html>
//10-15
//i=1; 2; 3; 4; 5; 6
//C=1<=5; 2<=5; 3<=5; 4<=5; 5<=5; 6<=5 #f
//u=i++ =i=i+; 1+1=2; 3; 4; 5; 5+1=6
//b=1; 2; 3; 4; 5
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <script>
        for(let i=2;i<=9;i+2){
            console.log(i+3);
        }
    </script>
</body>
</html>

  /*i=2; 2<=9; i=2+2=4 -----2+3=5
            i=4; 4<=9; i=4+2=6
            -----4+3=7
            i=6; 6<=9; i=6+2=8
            -----6+3=9
            i=8; 8<=8; i=8+2=10
            -----8+3=11*/

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <script>
        // for(let i=10;i<=15;i++){
        //     console.log(i);
        // }
        // for(let i=2;i<=9;i+2){
        //     console.log(i+3);
        // }
          for(let i=1;i<=5;i++){
             console.log("hi");
             document.write("hi"+"<br>")
         }
        // for(let i=1;i<=10;i++){
        //     let a=9*i
        //     document.write(a+"<br>")
        // }
        for(let i=1;i<=10;i++){
            let a=9*i
            document.write(`9x${i}=${a}<br>`)
        }
    </script>
</body>
</html>

VS CODE:
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <script>
        // for(let i=10;i<=15;i++){
        //     console.log(i);
        // }
        // for(let i=2;i<=9;i+2){
        //     console.log(i+3);
        // }
          for(let i=1;i<=5;i++){
             console.log("hi");
             document.write("hi"+"<br>")
         }
        // for(let i=1;i<=10;i++){
        //     let a=9*i
        //     document.write(a+"<br>")
        // }
        for(let i=1;i<=10;i++){
            let a=9*i
            document.write(`9x${i}=${a}<br>`)
        }
        const numbers=[1,2,3,4,5]
        let sum=0
        for(let i=0;i<numbers.length;i++){
            sum=sum+numbers[i]
            document.write(sum)
        }
    </script>
</body>
</html>
